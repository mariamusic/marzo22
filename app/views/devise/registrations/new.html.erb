<br><br><br><br><br>
<section class="overlay">
      <div class="container">


      <div class="col-md-6 col-md-offset-3 col-sm-6 col-sm-offset-3">
       <div class="row center">
          <h1>REGISTRATE</h1>

      </div>


      <%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
        <%= devise_error_messages! %>



       <div class="row center">
        <div class="form-group">
          <label for="exampleInputEmail1">E-mail</label>
          <%= f.email_field :email, autofocus: true, class: "form-control", id: "exampleInputEmail1" %>
         
        </div>
      </div>

       <div class="row center">
        <div class="form-group">
          <label for="exampleInputEmail1">Contraseña</label>
           <% if @validatable %>
          <em>(<%= @minimum_password_length %> characters minimum)</em>
          <% end %><br />
          <%= f.password_field :password, autocomplete: "off", class: "form-control", id: "exampleInputEmail1"  %>
          
         
        </div>
      </div>

      <div class="row center">
        <div class="form-group">
          <label for="exampleInputEmail1">Confirma tu contraseña</label>
          <%= f.password_field :password_confirmation, autocomplete: "off", class: "form-control", id: "exampleInputEmail1"   %>
        </div>
      </div>

    <div class="row center">
       
          <%= f.submit "Regístrate", class: "btn btn-primary"  %>
        </div>
      <% end %>

    <!--   <%= render "devise/shared/links" %> -->

      </div>
      </div>
</section>
</br></br></br></br></br></br><br>
</br></br>
